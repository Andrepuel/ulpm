function ulpm_pack_installed {
	ulpm_check_command "imlib2-config --version"
	return $?
}

function ulpm_pack_fetch {
	ulpm_gzget http://ufpr.dl.sourceforge.net/project/enlightenment/imlib2-src/1.4.4/imlib2-1.4.4.tar.gz imlib2-1.4.4
}

function ulpm_pack_install {
	ulpm_dotconfigure
	ulpm_make_install
	return $?
}

ulpm_pack_dependency=(make gzip)
