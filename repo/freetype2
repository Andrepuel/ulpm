function ulpm_pack_installed {
	ulpm_link_with freetype
	return $?
}

function ulpm_pack_fetch {
	ulpm_bz2get http://download.savannah.gnu.org/releases/freetype/freetype-2.4.8.tar.bz2 freetype-2.4.8
}

function ulpm_pack_install {
	ulpm_dotconfigure
	ulpm_make_install
	return $?
}

function ulpm_pack_uninstall {
	ulpm_dotconfigure
	make uninstall
}

ulpm_pack_dependency=(make bzip2)
