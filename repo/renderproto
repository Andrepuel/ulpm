function ulpm_pack_installed {
	ulpm_test_include "<X11/extensions/renderproto.h>"
	return $?
}

function ulpm_pack_fetch {
	ulpm_bz2get http://www.x.org/releases/X11R7.6/src/everything/renderproto-0.11.1.tar.bz2 renderproto-0.11.1
}

function ulpm_pack_install {
	ulpm_dotconfigure
	ulpm_make_install
	return $?
}

ulpm_pack_dependency=(make bzip2)
