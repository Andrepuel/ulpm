function ulpm_pack_installed {
	ulpm_check_command "gzip --version"
	return $?
}

function ulpm_pack_fetch {
	ulpm_get http://gnu.c3sl.ufpr.br/ftp/gzip/gzip-1.3.12.tar gzip-1.3.12
}

function ulpm_pack_install {
	ulpm_cant_install
	return $?
}

function ulpm_pack_uninstall {
	ulpm_cant_install
	return $?
}

ulpm_pack_dependency=()
